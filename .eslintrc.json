{
  "extends": [
    "next/core-web-vitals",
    "plugin:storybook/recommended",
    "prettier"
  ],
  "plugins": ["simple-import-sort", "prettier"],
  "rules": {
    "logical-assignment-operators": "error",
    "react/jsx-newline": "error",
    "react/self-closing-comp": [
      "error",
      {
        "component": true,
        "html": true
      }
    ],
    "simple-import-sort/imports": "error"
  }
}
